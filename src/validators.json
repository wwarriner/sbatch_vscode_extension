{
  "number_list": { "regex": "^(?:[1-9][0-9]*(?:-[1-9][0-9]*)?,?)*$" },
  "boolean_word_list": { "regex": "^([[:word:]]*[\\||\\$}?)*$" },
  "time": {
    "format": [
      "%m%d%y",
      "%m/%d/%y",
      "%Y-%m-%d",
      "%Y-%m-%dT%H:%M",
      "%Y-%m-%dT%H:%M:%S"
    ]
  },
  "delay": { "regex": "^now(?:\\+(?:second|minute|hour|day|week)s)?$" },
  "literal": { "regex": "^[[:word:]]*$" },
  "arg_list": {
    "regex": "^(?:[[:word:]]*(?:=[[:word:]]*)?)(?:,(?:[[:word:]]*(?:=[[:word:]]*)?))*$"
  },
  "group": { "regex": "^[[:word:]]*$" },
  "gpu": {
    "regex": "^[A-Za-z]+(?:\\:[1-9][0-9]*)?(?:,[A-Za-z]+(?:\\:[1-9][0-9]*)?)*$"
  },
  "gres": {
    "regex": "^[A-Za-z]+(?:(?:\\:[A-Za-z]+)?\\:[1-9][0-9]*)?(?:,[A-Za-z]+(?:(?:\\:[A-Za-z]+)?\\:[1-9][0-9]*)?)*$"
  },
  "license": {
    "regex": "^[[:word:]]+(?:@[[:word:]]+)?(?::[1-9][0-9]*)?(?:,[[:word:]]+(?:@[[:word:]]+)?(?::[1-9][0-9]*)?)*$"
  },
  "memory": {
    "regex": "^[1-9][0-9]*(?:K|M|G|T)?$"
  },
  "range": {
    "regex": "^[1-9][0-9]*(?:-[1-9][0-9]*)$"
  },
  "numeric_bool": {
    "regex": "^(?:0|1){1}$"
  }
}
